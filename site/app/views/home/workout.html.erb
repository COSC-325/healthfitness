<section class="py-5 rounded" style="margin-left: 10%; margin-right: 10%; padding-left: 10%; padding-right: 10%; margin-top: 5%;">
</section>
<div>
  <div class="flex-container">
    <div class="column text-center rounded">
      <h1 class="display-4">BMI Calculator</h1>
      <br>
      <p class="lead">Weight (Pounds)
        <input class="form-control" type="text" id="bmiWeight"/></p>
      <p class="lead">Height (Inches)
        <input class="form-control" type="text" id="bmiHeight"/></p>
      <input class="btn btn-primary" type="submit" value="Compute BMI" onclick="getBMI();">
      <br>
      <br>
      <p class="lead"><strong>BMI:
        <span id="bmiAnswer">NaN</span></strong>
      </p>
    </div>

    <hr style="padding: 5%; clear: both; visibility: hidden;">

    <div class="column text-center rounded">
      <h1 class="display-4">Calorie Calculator</h1>
      <br>
      <p class="lead">Age
        <input class="form-control" type="text" id="ccAge"/>
      </p>
      <p class="lead">Gender:
        <label><input type="radio" name="ccGender" id="male" checked>Male</label>
        <label><input type="radio" name="ccGender" id="female">Female</label>
      </p>
      <p class="lead">Weight (Pounds)
        <input class="form-control" type="text" id="ccWeight"/>
      </p>
      <p class="lead">Height (Inches)
        <input class="form-control" type="text" id="ccHeight"/>
      </p>
      <input class="btn btn-primary" type="submit" value="Compute Calories" onclick="getCalorie();">
      <br>
      <br>
      <p class="lead"><strong>BMR:
        <span id="ccAnswer">NaN</span> Calories/day</strong>
      </p>
    </div>
  </div>
</div>
<br>
<br>


<script type="text/javascript">
  function getBMI() {
    var weight = Number(document.getElementById("bmiWeight").value);
    var height = Number(document.getElementById("bmiHeight").value);
    var answer = (703 * weight) / (Math.pow(height, 2));
    document.getElementById("bmiAnswer").innerText = Math.round(answer * 100) / 100;
  }

function getCalorie() {
    var age = Number(document.getElementById("ccAge").value);
    var height = Number(document.getElementById("ccHeight").value) * 2.54;
    var weight = Number(document.getElementById("ccWeight").value) / 2.205;
    console.log(weight);
    var answer;
    if (document.getElementById('male').checked) {
      answer = (10 * weight) + (6.25 * height) - (5 * age) + 5;
    } else if (document.getElementById('female').checked) {
      answer = (10 * weight) + (6.25 * height) - (5 * age) - 161;
    }
  document.getElementById("ccAnswer").innerText = Math.round(answer);
  }
</script>
